{
  "name": "try_tetris",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "bower-installer": "^1.2.0",
    "eslint": "^1.2.0",
    "jasmine-node": "^1.14.5",
    "jsdoc": "^3.3.2",
    "karma": "^0.13.9",
    "karma-cli": "^0.1.0",
    "karma-coverage": "^0.5.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-requirejs": "^0.2.2",
    "nodemon": "^1.4.1",
    "phantomjs": "^1.9.18",
    "request": "^2.61.0",
    "requirejs": "^2.1.20",
    "squirejs": "^0.2.1"
  },
  "scripts": {
    "test_client": "karma start karma.client.conf.js --reporters progress",
    "test_server": "jasmine-node --captureExceptions server/",
    "test": "npm run test_server && npm run test_client",
    "doc_client": "rm -rf documentation && jsdoc -d documentation/ app/src/",
    "doc_server": "rm -rf documentation_server && jsdoc -d documentation_server/ server/",
    "doc": "npm run doc_server && npm run doc_client",
    "lint_client": "eslint app/src/",
    "lint_server": "eslint server/",
    "lint": "npm run lint_server && npm run lint_client",
    "buildJs": "rjs -o app/build.js",
    "buildCss": "rjs -o app/build-css.js",
    "build": "npm run buildJs && npm run buildCss",
    "renameRJs": "mv node_modules/.bin/r.js.cmd node_modules/.bin/rjs.cmd && mv node_modules/.bin/r.js node_modules/.bin/rjs",
    "postinstall": "bower install && bower-installer && npm run renameRJs",
    "prestart": "npm run lint && npm run build && npm run test",
    "server": "babel-node server.js",
    "start": "npm run server",
    "develop": "nodemon --watch server --exec npm run server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel": "^5.8.23",
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "mongoose": "^4.1.3",
    "morgan": "^1.6.1",
    "serve-favicon": "^2.3.0"
  }
}
