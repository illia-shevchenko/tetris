{
  "name": "try_tetris",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "bower-installer": "^1.2.0",
    "eslint": "^1.2.0",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "jasmine-node": "^2.0.0-beta4",
    "jsdoc": "^3.3.2",
    "karma": "^0.13.9",
    "karma-cli": "^0.1.0",
    "karma-coverage": "^0.5.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-requirejs": "^0.2.2",
    "phantomjs": "^1.9.18",
    "request": "^2.61.0",
    "requirejs": "^2.1.20",
    "squirejs": "^0.2.1"
  },
  "scripts": {
    "test": "karma start --reporters progress",
    "doc": "rm -rf documentation && jsdoc -d documentation/ app/src/",
    "lint": "eslint app/src/",
    "buildJs": "rjs -o app/build.js",
    "buildCss": "rjs -o app/build-css.js",
    "build": "npm run buildJs && npm run buildCss",
    "renameRJs": "mv node_modules/.bin/r.js.cmd node_modules/.bin/rjs.cmd && mv node_modules/.bin/r.js node_modules/.bin/rjs",
    "postinstall": "bower install && bower-installer && npm run renameRJs",
    "prestart": "npm run test && npm run lint && npm run build",
    "start": "babel-node server.js"
  },
